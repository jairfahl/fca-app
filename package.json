{
  "name": "fca-mtr",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=apps/api\" \"npm run dev --workspace=apps/web\"",
    "install:all": "npm install",
    "db:migrate": "node db/run-migrations.js",
    "db:seed": "node db/seed/seed-recommendations.js && node db/seed/seed-full-catalog.js && node db/seed/seed-cause-taxonomy.js && node db/seed/seed-cause-mvp.js",
    "db:seed:f3": "node db/seed/seed-f3-recommendations.js",
"db:seed:full": "node db/seed/seed-full-catalog.js",
    "db:seed:cause": "node db/seed/seed-cause-taxonomy.js",
    "db:seed:cause-mvp": "node db/seed/seed-cause-taxonomy.js && node db/seed/seed-cause-mvp.js",
    "catalog:validate:full": "node db/validate/validate_full_catalog.js",
    "e2e:full": "node scripts/e2e-full-flow-with-cause.js",
    "e2e:activate-full": "node scripts/e2e-activate-full.js",
    "e2e:reports": "node scripts/e2e-reports-versions-roles.js",
    "auth:diagnose": "node scripts/auth-diagnose.mjs",
    "auth:bootstrap": "node scripts/auth-bootstrap.mjs"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "dotenv": "^16.3.1"
  }
}
