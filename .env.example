# Database
DATABASE_URL=

# Supabase
SUPABASE_URL=
SUPABASE_ANON_KEY=
# Service Role: usado no backend para leitura transversal (ex: /consultor/companies). Nunca expor ao frontend.
SUPABASE_SERVICE_ROLE_KEY=

# Server
PORT=3001
NODE_ENV=development

# FULL - Bypass paywall para testes (monetização em stand-by)
# FULL_TEST_MODE: quando true, qualquer usuário pode acessar FULL sem pagamento
FULL_TEST_MODE=false
# FULL_ADMIN_WHITELIST: emails separados por vírgula (ex: admin@fca.com,fca@fca.com)
FULL_ADMIN_WHITELIST=admin@fca.com

# Role vem de app_metadata no Supabase (scripts/fix-auth-users-roles.sql). Não usar inferência por email.
